<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>col_studio.ico</ApplicationIcon>
    <PackageIcon>col_studio.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="col_studio.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="col_studio.ico" />
    <Resource Include="col_studio.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </Resource>
  </ItemGroup>

</Project>
